{
  "id": null,
  "title": "EventStore Load Generator Dashboard",
    "tags": ["eventstore", "load-generator", "performance"],
    "timezone": "browser",
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Load Generator Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "load_generator_current_rate",
            "legendFormat": "Current Rate (req/s)",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 20},
                {"color": "green", "value": 25}
              ]
            }
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        }
      },
      {
        "id": 2,
        "title": "Scenario Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "rate(load_generator_scenarios_total[1m])",
            "legendFormat": "{{scenario_type}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
        "options": {
          "reduceOptions": {"calcs": ["lastNotNull"]},
          "pieType": "pie",
          "tooltip": {"mode": "single"}
        }
      },
      {
        "id": 3,
        "title": "Success vs Error Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(load_generator_scenarios_total[1m])",
            "legendFormat": "Total Requests/s",
            "refId": "A"
          },
          {
            "expr": "rate(load_generator_errors_total[1m])",
            "legendFormat": "Errors/s",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 4,
        "title": "Load Generator Request Duration (P50, P95, P99)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(load_generator_request_duration_seconds_bucket[1m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(load_generator_request_duration_seconds_bucket[1m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(load_generator_request_duration_seconds_bucket[1m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 5,
        "title": "EventStore Operation Duration (P50, P95, P99)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(eventstore_query_duration_seconds_bucket[1m]))",
            "legendFormat": "Query P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(eventstore_query_duration_seconds_bucket[1m]))",
            "legendFormat": "Query P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(eventstore_query_duration_seconds_bucket[1m]))",
            "legendFormat": "Query P99",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.50, rate(eventstore_append_duration_seconds_bucket[1m]))",
            "legendFormat": "Append P50",
            "refId": "D"
          },
          {
            "expr": "histogram_quantile(0.95, rate(eventstore_append_duration_seconds_bucket[1m]))",
            "legendFormat": "Append P95",
            "refId": "E"
          },
          {
            "expr": "histogram_quantile(0.99, rate(eventstore_append_duration_seconds_bucket[1m]))",
            "legendFormat": "Append P99",
            "refId": "F"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 6,
        "title": "Error Breakdown by Type",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(load_generator_errors_total[1m])",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 7,
        "title": "EventStore Operations Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(eventstore_events_queried_total[1m])",
            "legendFormat": "Events Queried/s",
            "refId": "A"
          },
          {
            "expr": "rate(eventstore_events_appended_total[1m])",
            "legendFormat": "Events Appended/s",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 8,
        "title": "Scenario Execution Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(load_generator_scenarios_total{scenario_type=\"circulation\"}[1m])",
            "legendFormat": "Circulation (4%)",
            "refId": "A"
          },
          {
            "expr": "rate(load_generator_scenarios_total{scenario_type=\"lending\"}[1m])",
            "legendFormat": "Lending (94%)",
            "refId": "B"
          },
          {
            "expr": "rate(load_generator_scenarios_total{scenario_type=\"error\"}[1m])",
            "legendFormat": "Error (2%)",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 22},
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 9,
        "title": "Concurrency Conflicts Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(load_generator_errors_total{error_type=\"concurrency_conflict\"}[1m])",
            "legendFormat": "Concurrency Conflicts/s",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 30},
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 1.0}
              ]
            }
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        }
      },
      {
        "id": 10,
        "title": "Total Requests Processed",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(load_generator_scenarios_total)",
            "legendFormat": "Total Requests",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 30},
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value"
        }
      },
      {
        "id": 11,
        "title": "Load Generator Performance Summary",
        "type": "table",
        "targets": [
          {
            "expr": "load_generator_current_rate",
            "legendFormat": "Current Rate (req/s)",
            "refId": "A",
            "format": "table"
          },
          {
            "expr": "sum(rate(load_generator_scenarios_total[1m]))",
            "legendFormat": "Actual Rate (req/s)",
            "refId": "B",
            "format": "table"
          },
          {
            "expr": "sum(load_generator_scenarios_total)",
            "legendFormat": "Total Requests",
            "refId": "C",
            "format": "table"
          },
          {
            "expr": "sum(load_generator_errors_total)",
            "legendFormat": "Total Errors",
            "refId": "D",
            "format": "table"
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 30},
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"}
          }
        }
      }
    ]
}