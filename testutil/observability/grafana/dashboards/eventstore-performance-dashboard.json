      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          },
          "expr": "rate(eventstore_component_duration_seconds_sum{operation=\"query\",status=\"success\"}[1m]) / rate(eventstore_component_duration_seconds_count{operation=\"query\",status=\"success\"}[1m]) * 1000000",
          "legendFormat": "{{component}}",
