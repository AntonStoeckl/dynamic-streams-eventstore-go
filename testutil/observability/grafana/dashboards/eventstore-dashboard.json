      "id": 1,
      "title": "Successful EventStore.Append()/sec",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "PBFA97CFB590B2093"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          },
          "expr": "rate(eventstore_append_method_duration_seconds_count{status=\"success\"}[1m])",
          "legendFormat": "Successful Appends/sec",
          "refId": "A"
        }
      ],
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
