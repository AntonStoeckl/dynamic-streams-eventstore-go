{
  "id": null,
  "title": "EventStore Test Load Dashboard",
    "tags": ["eventstore", "test"],
    "timezone": "browser",
    "time": {
      "from": "now-5m",
      "to": "now"
    },
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Operation Duration (P50, P95, P99)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(eventstore_query_duration_seconds_bucket[1m]))",
            "legendFormat": "Query P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(eventstore_query_duration_seconds_bucket[1m]))",
            "legendFormat": "Query P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(eventstore_query_duration_seconds_bucket[1m]))",
            "legendFormat": "Query P99",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.50, rate(eventstore_append_duration_seconds_bucket[1m]))",
            "legendFormat": "Append P50",
            "refId": "D"
          },
          {
            "expr": "histogram_quantile(0.95, rate(eventstore_append_duration_seconds_bucket[1m]))",
            "legendFormat": "Append P95",
            "refId": "E"
          },
          {
            "expr": "histogram_quantile(0.99, rate(eventstore_append_duration_seconds_bucket[1m]))",
            "legendFormat": "Append P99",
            "refId": "F"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0
          }
        }
      },
      {
        "id": 2,
        "title": "Operations Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(eventstore_events_queried_total[1m])",
            "legendFormat": "Query Rate",
            "refId": "A"
          },
          {
            "expr": "rate(eventstore_events_appended_total[1m])",
            "legendFormat": "Append Rate",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "min": 0
          }
        }
      },
      {
        "id": 3,
        "title": "Error Rates",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(eventstore_database_errors_total[1m])",
            "legendFormat": "Database Errors",
            "refId": "A"
          },
          {
            "expr": "rate(eventstore_concurrency_conflicts_total[1m])",
            "legendFormat": "Concurrency Conflicts",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "min": 0
          }
        }
      },
      {
        "id": 4,
        "title": "Events Processed",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(eventstore_events_queried_total)",
            "legendFormat": "Total Events Queried",
            "refId": "A"
          },
          {
            "expr": "sum(eventstore_events_appended_total)",
            "legendFormat": "Total Events Appended",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      }
    ]
}