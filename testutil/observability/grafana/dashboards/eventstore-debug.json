{
  "id": null,
  "title": "EventStore Debug Dashboard",
  "tags": ["eventstore", "debug", "operations"],
  "timezone": "browser",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "refresh": "5s",
  "panels": [
    {
      "id": 1,
      "title": "Append Operations Breakdown",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(eventstore_append_duration_seconds_count{status=\"success\"}[1m])",
          "legendFormat": "Successful Appends/sec",
          "refId": "A"
        },
        {
          "expr": "rate(eventstore_append_duration_seconds_count{status=\"error\"}[1m])",
          "legendFormat": "Failed Appends/sec",
          "refId": "B"
        },
        {
          "expr": "rate(eventstore_append_duration_seconds_count[1m])",
          "legendFormat": "Total Appends/sec",
          "refId": "C"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 2,
      "title": "Query Operations Breakdown",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(eventstore_query_duration_seconds_count{status=\"success\"}[1m])",
          "legendFormat": "Successful Queries/sec",
          "refId": "A"
        },
        {
          "expr": "rate(eventstore_query_duration_seconds_count{status=\"error\"}[1m])",
          "legendFormat": "Failed Queries/sec",
          "refId": "B"
        },
        {
          "expr": "rate(eventstore_query_duration_seconds_count[1m])",
          "legendFormat": "Total Queries/sec",
          "refId": "C"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 3,
      "title": "Method Calls vs SQL Operations",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(eventstore_append_method_calls_total[1m])",
          "legendFormat": "EventStore.Append() Method Calls/sec",
          "refId": "A"
        },
        {
          "expr": "rate(eventstore_query_method_calls_total[1m])",
          "legendFormat": "EventStore.Query() Method Calls/sec",
          "refId": "B"
        },
        {
          "expr": "rate(eventstore_append_duration_seconds_count[1m])",
          "legendFormat": "SQL INSERT Operations/sec",
          "refId": "C"
        },
        {
          "expr": "rate(eventstore_query_duration_seconds_count[1m])",
          "legendFormat": "SQL SELECT Operations/sec",
          "refId": "D"
        }
      ],
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8},
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 4,
      "title": "Additional Metrics",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(eventstore_concurrency_conflicts_total[1m])",
          "legendFormat": "Concurrency Conflicts/sec",
          "refId": "A"
        },
        {
          "expr": "rate(eventstore_database_errors_total[1m])",
          "legendFormat": "Database Errors/sec",
          "refId": "B"
        },
        {
          "expr": "rate(eventstore_events_queried_total[1m])",
          "legendFormat": "Events Queried/sec (gauge)",
          "refId": "C"
        },
        {
          "expr": "rate(eventstore_events_appended_total[1m])",
          "legendFormat": "Events Appended/sec (gauge)",
          "refId": "D"
        }
      ],
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {"mode": "palette-classic"}
        }
      }
    }
  ],
  "schemaVersion": 36,
  "version": 0,
  "weekStart": ""
}